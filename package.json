{
  "name": "treetrunk",
  "version": "0.0.1",
  "description": "Seller managament system that process payroll",
  "main": "dist/index.js",
  "author": "untritt and unmyke",
  "license": "MIT",
  "scripts": {
    "build:client": "webpack --env.target=client --env.mode=production",
    "build:server": "webpack --env.target=server --env.mode=production",
    "build": "yarn run build:client && yarn run build:server",
    "build-dev:client": "webpack --env.target=client --env.mode=development",
    "build-dev:server": "webpack --env.target=server --env.mode=development",
    "build-dev": "yarn run build-dev:client && yarn run build-dev:server",
    "watch": "webpack --env.target=server --env.mode=development --watch",
    "nodemon": "nodemon dist/server/development.bundle.js --watch dist/server",
    "dev:client": "webpack-dev-server --env.target=client --env.mode=development",
    "dev:server": "concurrently --kill-others \"npm:watch\" \"npm:nodemon\"",
    "dev": "concurrently --kill-others \"npm:dev:*\"",
    "start": "yarn run build && node dist/server/production.bundle.js",
    "console": "NODE_PATH=. yarn run babel-node src/server/interfaces/console/index.js",
    "jest": "yarn test",
    "test": "NODE_PATH=. NODE_ENV=test jest --testURL='http://localhost/'",
    "test:all": "yarn run test:unit && yarn run test:features",
    "test:unit": "yarn run jest 'src/.*\\.unit\\.test\\.js'",
    "test:features": "yarn run jest 'src/.*\\.feature\\.test\\.js'",
    "lint": "eslint {src,config}/**/*.js",
    "pm2": "pm2",
    "precommit": "lint-staged",
    "prettier": "prettier",
    "debug": "node --debug-brk node_modules/jest/bin/jest --runInBand --no-cache link"
  },
  "jest": {
    "setupFiles": [
      "jest-plugin-context/setup"
    ]
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/plugin-proposal-do-expressions": "^7.2.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-proposal-function-sent": "^7.2.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.2.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.2.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.2.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.3",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "jest-plugin-context": "^2.9.0",
    "lint-staged": "^8.1.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-middleware": "^3.5.0",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@material-ui/core": "^3.8.3",
    "@material-ui/icons": "^3.0.2",
    "apollo-boost": "^0.1.23",
    "apollo-client": "^2.4.8",
    "bottlejs": "^1.7.1",
    "classnames": "^2.2.6",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "date-fns": "^1.30.1",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-status-monitor": "^1.2.3",
    "graphql": "^14.0.2",
    "graphql-client": "^2.0.1",
    "http-status": "^1.3.1",
    "javascript-state-machine": "^3.1.0",
    "lodash": "^4.17.11",
    "log4js": "^3.0.6",
    "method-override": "^3.0.0",
    "modern-normalize": "^0.5.0",
    "mongorito": "^3.0.4",
    "morgan": "^1.9.1",
    "pm2": "^3.2.8",
    "react": "^16.7.0",
    "react-apollo": "^2.3.3",
    "react-dom": "^16.7.0",
    "react-router-dom": "^4.3.1",
    "request": "^2.34",
    "supertest": "^3.3.0",
    "swagger-ui-express": "^4.0.2",
    "typeface-roboto": "^0.0.54",
    "validate.js": "^0.12.0"
  }
}
