{
  "name": "treetrunk",
  "version": "0.0.1",
  "description": "Seller managament system that process payroll",
  "main": "dist/index.js",
  "author": "untritt and unmyke",
  "license": "MIT",
  "scripts": {
    "babel-node": "babel-node",
    "build": "babel src --out-dir dist --source-maps",
    "compile": "yarn build --watch",
    "start": "nodemon --exec node -- ./cluster.js",
    "dev": "NODE_PATH=. NODE_ENV=development nodemon --exec yarn run babel-node ./src/index.js",
    "console": "NODE_PATH=. yarn run babel-node src/interfaces/console/index.js",
    "jest": "yarn test",
    "test": "NODE_PATH=. NODE_ENV=test jest --testURL='http://localhost/'",
    "test:all": "yarn run test:unit && yarn run test:features",
    "test:unit": "yarn run jest 'src/.*\\.unit\\.test\\.js'",
    "test:features": "yarn run jest 'src/.*\\.feature\\.test\\.js'",
    "lint": "eslint {src,config}/**/*.js",
    "pm2": "pm2",
    "precommit": "lint-staged",
    "prettier": "prettier",
    "debug": "node --debug-brk node_modules/jest/bin/jest --runInBand --no-cache link"
  },
  "jest": {
    "setupFiles": [
      "jest-plugin-context/setup"
    ]
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/plugin-proposal-do-expressions": "^7.2.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-proposal-function-sent": "^7.2.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.2.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.2.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.10.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.3.0",
    "jest": "^23.6.0",
    "jest-plugin-context": "^2.9.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.3",
    "bottlejs": "^1.7.1",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "date-fns": "^1.30.1",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-status-monitor": "^1.2.3",
    "http-status": "^1.3.1",
    "javascript-state-machine": "^3.1.0",
    "lodash": "^4.17.11",
    "log4js": "^3.0.6",
    "method-override": "^3.0.0",
    "mongorito": "^3.0.4",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.9",
    "pm2": "^3.2.4",
    "supertest": "^3.3.0",
    "swagger-ui-express": "^4.0.2",
    "validate.js": "^0.12.0"
  }
}
