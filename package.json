{
  "name": "treetrunk",
  "version": "0.0.1",
  "description": "Seller managament system that process payroll",
  "main": "dist/index.js",
  "author": "untritt and unmyke",
  "license": "MIT",
  "scripts": {
    "jest": "yarn test",
    "test": "NODE_PATH=. NODE_ENV=test jest",
    "test:all": "yarn run test:unit && yarn run test:features",
    "test:unit": "yarn run jest 'src/.*\\.unit\\.test\\.js'",
    "test:features": "yarn run jest 'src/.*\\.feature\\.test\\.js'",
    "babel-node": "babel-node",
    "build": "NODE_ENV=production babel src --out-dir dist --source-maps",
    "start": "nodemon --exec yarn run babel-node -- ./cluster.js",
    "dev":
      "NODE_PATH=. NODE_ENV=development nodemon --exec yarn run babel-node -- ./index.js",
    "lint": "eslint {src,config}/**/*.js",
    "sequelize": "NODE_PATH=. sequelize",
    "sequelize:migrate":
      "yarn run sequelize db:migrate --env development && yarn run sequelize db:migrate --env test",
    "sequelize:migrate:undoAll":
      "yarn run sequelize db:migrate:undo:all --env development && yarn run sequelize db:migrate:undo:all --env test",
    "sequelize:re-migrate":
      "yarn run sequelize:migrate:undoAll && yarn run sequelize:migrate",
    "console": "NODE_PATH=. babel-node src/interfaces/console/index.js",
    "pm2": "pm2",
    "precommit": "lint-staged",
    "prettier": "prettier"
  },
  "jest": {
    "setupFiles": ["jest-plugin-context/setup"]
  },
  "lint-staged": {
    "*.{js,json,css,md}": ["prettier --write", "git add"]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "factory-girl": "^5.0.2",
    "factory.js": "^0.0.8",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "jest-plugin-context": "^2.9.0",
    "lint-staged": "^7.0.4",
    "prettier": "^1.12.1",
    "sequelize-cli": "^4.0.0"
  },
  "dependencies": {
    "bottle": "^0.2.0",
    "bottlejs": "^1.7.0",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "date-fns": "^1.29.0",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "express-status-monitor": "^1.0.1",
    "http-status": "^1.0.1",
    "log4js": "^2.5.3",
    "method-override": "^2.3.10",
    "morgan": "^1.9.0",
    "mysql2": "^1.5.3",
    "nodemod": "^1.5.19",
    "nodemon": "^1.17.3",
    "pm2": "^2.10.2",
    "sequelize": "^4.37.6",
    "supertest": "^3.0.0",
    "swagger-ui-express": "^3.0.8",
    "validate.js": "^0.12.0"
  }
}
