{
  "name": "treetrunk",
  "version": "0.0.1",
  "description": "Saler managament system that process payroll",
  "main": "index.js",
  "author": "unmyke",
  "license": "MIT",
  "scripts": {
    "jest": "yarn test",
    "test": "NODE_PATH=. NODE_ENV=test jest",
    "test:all": "yarn run test:unit && yarn run test:features",
    "test:unit": "yarn run jest 'src/.*\\.unit\\.test\\.js'",
    "test:features": "yarn run jest 'src/.*\\.feature\\.test\\.js'",
    "babel-node": "babel-node",
    "build": "NODE_ENV=production babel src --out-dir dist --source-maps"
  },
  "jest": {
    "setupFiles": [
      "jest-plugin-context/setup"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.19.1",
    "jest": "^22.4.3",
    "jest-plugin-context": "^2.9.0",
    "prettier": "1.12.0"
  },
  "dependencies": {
    "date-fns": "^1.29.0"
  }
}
